{
  "name": "cytosis",
  "version": "0.0.5",
  "description": "Cytosis airtable CMS helper",
  "author": "Jan Zheng",
  "homepage": "https://github.com/janzheng/cytosis",
  "main": "./dist/cytosis.js",
  "files": [
    "/dist",
    "/src",
    "readme.md"
  ],
  "scripts": {
    "----notes": "If you're just updating cytosis, run 'npm run prod' (yarn prod won't work, and not npm publish, as this builds some of the scripts); For updating both the CDN and NPM, run 'npm run deploy'. Cheers",
    "surge": "cd cdn; surge . -d cytosis-core.surge.sh",
    "dev": "node build/dev.js",
    "build": "node build/build.js",
    "commit": "git add . -A; git commit; git push origin master",
    "babel": "babel src --out-dir dist",
    "babel2": "babel src --presets=stage-2 --plugins=babel-transform-object-rest-spread,transform-class-properties --out-dir dist",
    "babelold": "babel src --presets babel-preset-es2015 --plugins=transform-object-rest-spread,transform-class-properties --out-dir dist",
    "cdn": "npm run build; npm run babel; npm run surge;",
    "pub": "npm publish",
    "prod": "npm run babel; npm run pub",
    "site": "cp dist/cytosis.js site/source/assets/javascripts/cytosis.js; cd site; bundle exec middleman server --port 9001",
    "deploy": "npm run prod; npm run cdn"
  },
  "dependencies": {
    "airtable": "^0.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/plugin-transform-typeof-symbol": "^7.8.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "compression-webpack-plugin": "^1.1.6",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^4.0.4",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
